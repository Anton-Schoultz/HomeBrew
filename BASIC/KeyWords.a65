#ifdef hide
.NOLIST
#endif
#if (($/256)>=PG_ROM) & (($/256)<PG_KEY)
;=================================================================== KEYWORDS =========
;*  X_Nop                NO Op
;*
_KeyWords 	= $

;----------------------------------------------- X_KeyWords
X_Nop: ; NO Op
 		rts

.echo "  _KeyWords\t"
.echo $-_KeyWords
.echo "\n"
.nolist
#endif

;----------------------------------------------- keyword
#if ($/256) = PG_KEY
.list


W_AND 		.DB 	"AN",'D'+$80
W_OR 		.DB 	"O",'R'+$80
W_XOR 		.DB 	"XO",'R'+$80
W_NOT 		.DB 	"NO",'T'+$80


.nolist
#endif
;----------------------------------------------- run
#if ($/256) = PG_RUN
.list

T_AND 		= LOW($)+$80
.DW 	X_Nop

T_OR 		= LOW($)+$80
.DW 	X_Nop

T_XOR 		= LOW($)+$80
.DW 	X_Nop

T_NOT		= LOW($)+$80
.DW 	X_Nop


.nolist
;.export T_LE,T_GE,T_NE
#endif
